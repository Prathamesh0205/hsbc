generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model User{
  id String @id @default(uuid())
  customer String 
  age String
  zipCodeOri String
  merchants Merchant[] @relation("UserToMerchant")
 
}
model Merchant{
id String @id @default(uuid())
merchant String 
zipMerchant String
customers User[]@relation("UserToMerchant")
categories Categories[]@relation("MerchantToCategories")
}

model Categories{
id String @id @default(uuid())
category String 
amount Float
merchants Merchant[]@relation("MerchantToCategories")
}